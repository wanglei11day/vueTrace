<script type="text/javascript"><!--
    jQuery('#button-upload').on('click', function() {
        jQuery('#form-upload').remove();

        jQuery('body').prepend('<form enctype="multipart/form-data" id="form-upload" style="display: none;"><input type="file" name="file" value="" /></form>');

        jQuery('#form-upload input[name=\'file\']').trigger('click');

        if (typeof timer != 'undefined') {
            clearInterval(timer);
        }

        timer = setInterval(function() {
            if (jQuery('#form-upload input[name=\'file\']').val() != '') {
                clearInterval(timer);

                jQuery.ajax({
                    url: 'index.php?route=common/k_filemanager/upload&token=<?php echo $token; ?>&directory=<?php echo $directory; ?>',
                    type: 'post',
                    dataType: 'json',
                    data: new FormData(jQuery('#form-upload')[0]),
                    cache: false,
                    contentType: false,
                    processData: false,
                    xhr: function(){ //ajaxSettingsxhruploadprogress

                        myXhr = jQuery.ajaxSettings.xhr();
                        if(myXhr.upload){ //upload
                            //progress
                            myXhr.upload.addEventListener('progress',progressHandlingFunction, false);
                        }
                        return myXhr; //xhrjQuery
                    },
                    beforeSend: function() {
                        jQuery('.progress').show();
                        jQuery('#button-upload i').replaceWith('<i class="fa fa-circle-o-notch fa-spin"></i>');
                        jQuery('#button-upload').prop('disabled', true);
                    },
                    complete: function() {
                        jQuery('#button-upload i').replaceWith('<img src="image/data/upload32.png"/>');
                        jQuery('#button-upload').prop('disabled', false);
                        jQuery('.progress').hide();
                    },
                    success: function(json) {
                        if (json['error']) {
                            alert(json['error']);
                        }

                        if (json['success']) {
                            alert(json['success']);
                            jQuery('#button-refresh').trigger('click');
                        }
                    },
                    error: function(xhr, ajaxOptions, thrownError) {
                        alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                    }
                });
            }
        }, 500);
    });

    // 
    function progressHandlingFunction(e) {
        if (e.lengthComputable) {
            // jQuery('progress').attr({value : e.loaded, max : e.total}); //
            var percent = e.loaded/e.total*100;
            jQuery('.progress .progress-bar').css('width', percent+'%');
            // jQuery('#progress').html(e.loaded + "/" + e.total+" bytes. " + percent.toFixed(2) + "%");
        }

    }

